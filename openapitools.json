{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.1.0",
    "generators": {
      "api_reference": {
        "generatorName": "markdown",
        "output": "#{cwd}/src/app/api/reference",
        "glob": "./src/app/.vuepress/public/api/definitions/v1.0/open-api/apiDefinition.swagger.yaml"
      },
      "node_sdk": {
        "generatorName": "javascript",
        "output": "#{cwd}/src/app/api/sdks/node",
        "glob": "./src/app/.vuepress/public/api/definitions/v1.0/open-api/apiDefinition.swagger.yaml"
      },
      "powershell_sdk": {
        "generatorName": "powershell",
        "output": "#{cwd}/src/app/api/sdks/powershell",
        "glob": "./src/app/.vuepress/public/api/definitions/v1.0/open-api/apiDefinition.swagger.yaml"
      }
    }
  }
}